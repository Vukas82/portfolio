$plavaBoja:#03416e;
$tamnoPlavaBoja:#011126;
$sivaBoja: #929497;
$zutaBoja: #FEE934;








$textBoja: #ffffff;
// $textBoja: #212121;





@import "normalize";
@import "header";
@import "viewport";

*,
::before,
::after {
    margin: 0;
    padding: 0;
    text-align: center;
    box-sizing: inherit;
}



html {
    font-size: 62.5%;
}



body {
    font-family: 'Ginger_light', sans-serif;
    color: $sivaBoja;
    font-weight: 400;
    line-height: 1.7;
    box-sizing: border-box;

    // background-image: url("../images/POZADINA2_BEZ_BELE.png");
    background-image: linear-gradient(to left bottom, #011126, #34264f, #793266, #bd4064, #ef644c, #fd803c, #ffa028, #ffc110, #ffcb19, #ffd521, #ffdf2b, #fee934);

}



h2,
h3,
h4,
h5,
h6 {

    color: $plavaBoja;
    clear: both;
    line-height: 1.2;
    font-weight: 500;

}

.container {
    width: 100%;
    max-width: 111rem;
    margin: auto;
}








