            }


        }
    }

    .work1:hover {
        .mask {
            transform: scaleY(2);

            h4 {
                color: #ffffff;
                animation-name: moveRight2;

            }

            .cite {
                color: #ffffff;
                animation-name: moveRight2;
            }

            .span {
                animation-name: moveRight3;
            }
        }
    }


    .work2 {
