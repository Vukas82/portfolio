            }

            .span {
                display: inline-block;

                position: absolute;
                bottom: .8rem;
                right: 1rem;
                width: 4rem;
                height: .4rem;
                background-image: url(../images/next.svg);
                background-position: center;
                background-size: cover;
                background-repeat: no-repeat;
                color: rgba(255, 255, 255, 0); // opacity: 0;

            }


        }
    }

    .work2:hover {
        .mask {
            transform: scaleY(2);

            h4 {
                color: #ffffff;
                animation-name: moveRight2;

            }

            .cite {
                color: #ffffff;
