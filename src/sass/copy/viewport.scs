.viewPort__wraper {
    width: 100%;
    height: auto;
    position: relative;

    .animationText {
        height: 50rem;
        width: 100%;
        margin: auto;
        margin-top: 5rem;
        display: flex;
        flex-direction: row;
        justify-content: flex-start;
        align-items: center;
        border-radius: 1rem;

        svg {
            padding-top: 5rem;
            width: 55%;
        }


    }


    .makeing {
        height: 40rem;
        width: 100%;
        display: flex;
        flex-direction: row;
        align-items: center;
        justify-content: center;
        margin: auto;
        border-radius: .5rem;
        margin-top: 4rem;

        .makeing__text {
            color: rgba(33, 33, 33, 0.9);
            max-width: 80rem;
            margin: auto;

            h2 {
                text-align: center;

                color: rgba(33, 33, 33, 0.9);
                // color: #fff;

                font-size: 4.2rem;
                margin-bottom: 0.5em;
                line-height: 1.2em;
                letter-spacing: -0.04em;


            }

            p {

                // text-align: center;

                padding: 2rem 3rem;

                font-size: 1.6rem;
                line-height: 1.7em;
                letter-spacing: -0.02em;
                font-weight: 600;
                // font-family: 'Ginger Regular';
            }

            height: 80%;

            // width: 50%;
            .makeing__text__p {
                text-align: center;
                position: relative;
            }



            .makeing__text__p::after {
                content: '';
                position: absolute;
                background-color: #fffade;
                bottom: 0;
                left: 50%;
                width: 2.4rem;
                height: .7rem;
                margin: -.35rem 0 0 -1.2rem;

                transition: 0.72s cubic-bezier(0.52, 0.01, 0.16, 1);

                transform: skewY(-16deg) scaleX(1);

                transform-origin: 0 100%;

            }
        }


    }

    .how__make {
        height: 15rem;
        width: 100%;
        display: flex;
        flex-direction: row;
        align-items: center;
        justify-content: center;
        background-color: #f4f4f4;
        margin: auto;
        border-radius: 1rem;
        margin-top: 4rem;

        box-shadow: inset -.3rem -.3rem .2rem #ffffff73, inset .3rem .3rem .2rem #0b1d3d49;
    }
}

.portret {
    width: 40%;
    position: absolute;
    right: 0;
    top: 20rem;



    svg {
        width: 40rem;
        height: 40rem;
        margin: auto;

        #logo {

            transform: translateX(50rem) translateY(14rem);
            animation-duration: 3s;
            animation-name: animateLogo;
            animation-iteration-count: infinite;
            animation-direction: alternate;
            // mix-blend-mode: overlay;
        }
    }

    @keyframes animateLogo {
        from {
            transform: translateX(50rem) translateY(14rem);
            opacity: 1
        }

        to {
            transform: translateX(44.8rem) translateY(17.5rem) scale(0.98);
            opacity: 1
        }
    }
}

.section1 {
    position: relative;
    width: 100%;
    height: 60rem;
    background-color: transparent;
    overflow: hidden;

    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

    h3 {

        font-weight: bolder;
        z-index: 30;

        letter-spacing: -0.05em;
        line-height: 0.9em;
        font-size: 6.5vw;
        color: #fffade;
        margin-bottom: 0.35em;
    }

    .box__arrow__link {
        display: inline-block;
        height: 4.2rem;
        text-transform: uppercase;
        border: .2rem solid #fff;

        transition: 0.52s cubic-bezier(0.7, 0.3, 0, 1);

        transform: translateX(-2.2rem);

        font-size: 1.05rem;
        letter-spacing: 0.1em;

        .box__arrow__1 {
            position: absolute;
            height: 4.3rem;
            width: 5rem;

            top: -.2rem;
            overflow: hidden;

            right: 100%;

            .arrow_head {
                left: 50%;
                top: 50%;
                width: 2rem;
                margin: -1.4rem 0 0 -1rem;
                position: absolute;
                height: 2.6rem;
                overflow: hidden;

                span {


                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;

                    transition: 0.45s cubic-bezier(0.7, 0.3, 0, 1);

                    transform: translateX(-105%);
                }

                span::before {
                    height: .2rem;
                    width: 90%;
                    left: 0;
                    // left: 20px;
                    margin-top: -.1rem;
                    background-color: black;
                    content: '';
                    position: absolute;
                    top: 50%;
                }

                span::after {
                    content: '';
                    position: absolute;
                    top: 50%;
                    right: 0;
                    width: .6rem;
                    height: .6rem;
                    margin: -.3rem 0 0 -.3rem;
                    background: url(../images/arow_head_black.svg) no-repeat center center/100%;

                }
            }

        }

        .box__arrow__1::before {
            transform: translateX(105%);

            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 4.2rem;
            border-top: .2rem solid #fff;
            border-bottom: .2rem solid #fff;

            transition: 0.55s cubic-bezier(0.7, 0.3, 0, 1);

            border-left: .2rem solid #fff;

            transform-origin: 100% 0;
            background-color: #FFF;

            transition-delay: 0.22s;
        }




        .box__arrow__text {
            padding: 0 1.6rem;
            line-height: 3.6rem;

        }

        .box__arrow__2 {
            position: absolute;
            height: 4.3rem;
            width: 5rem;

            top: -.2rem;
            left: 100%;
            overflow: hidden;


            .arrow_head {
                left: 25%;
                top: 25%;
                width: 2rem;
                margin: -1.4 0 0 -1rem;
                position: absolute;
                height: 2.6rem;
                overflow: hidden;

                span {


                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    transition: 0.45s cubic-bezier(0.7, 0.3, 0, 1);
                }

                span::before {
                    height: .2rem;
                    width: 90%;
                    left: 0;
                    margin-top: -1px;
                    background-color: black;
                    content: '';
                    position: absolute;
                    top: 50%;
                }

                span::after {
                    content: '';
                    position: absolute;
                    top: 50%;
                    right: 0;
                    width: .6rem;
                    height: .6rem;
                    margin: -.3rem 0 0 -.3rem;
                    background: url(../images/arow_head_black.svg) no-repeat center center/100%;

                }
            }

        }

        .box__arrow__2::before {

            transform: translateX(-105%);

            content: url(../images/next_black.svg);
            background-position: center;
            background-size: cover;
            background-repeat: no-repeat;
            position: absolute;
            top: 0;
            left: 100%;
            width: 100%;
            height: 4.2rem;
            border-top: .2rem solid #fff;
            border-bottom: .2rem solid #fff;
            transition: 0.55s cubic-bezier(0.7, 0.3, 0, 1);
            border-left: .2rem solid #fff;
            transform-origin: 100% 0;
            transition-delay: 0.22s;

            margin-left: -.3rem;
            border-right: .2rem solid #fff;
            transform-origin: 0 0;
        }
    }

    .box__arrow__link:hover {
        transform: translateX(2.5rem);
        transition-delay: 0.15s;

        .box__arrow__1 {
            .arrow_head {
                span {
                    transform: translateX(0%);
                    transition-delay: 0.22s;
                }
            }

        }


        .box__arrow__1::before {
            transform: translateX(0%);
            transition-delay: 0.22s;
        }

        .box__arrow__text {
            transform: translateX(25px);
        }





        .box__arrow__2 {
            left: 100%;

            .arrow_head {
                span {
                    transform: translateX(0%); //bilo zakomentovano
                    transition-delay: 0.22s;
                    transform: translateX(-105%);
                }
            }

        }


        .box__arrow__2::before {
            transform: translateX(-105%);
            left: 0%;

        }
    }

    .line {
        width: 150%;
        background-color: $zutaBoja;
        height: 60rem;
        margin: auto;
        position: absolute;
        bottom: 0;
        left: -30%;
        opacity: 0;
    }

    .work1 {
        width: 36rem;
        height: 18rem;
        background-image: url('../images/prethodni_poslovi_1.1.png');
        background-position: center;
        background-size: cover;
        background-repeat: no-repeat;
